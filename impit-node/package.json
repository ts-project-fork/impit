{
  "name": "impit",
  "homepage": "https://apify.github.io/impit/js",
  "version": "0.7.1",
  "main": "index.wrapper.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "impit-node",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apify/impit.git"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.70",
    "@types/express": "^5.0.0",
    "@types/node": "^24.0.0",
    "express": "^5.0.0",
    "proxy-chain": "^2.5.9",
    "socks-server-lib": "^0.0.3",
    "tough-cookie": "^6.0.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-mdn-links": "^5.0.9",
    "typescript": "^5.9.2",
    "vitest": "^4.0.0"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "artifacts": "napi artifacts --output-dir ../artifacts --npm-dir npm",
    "build": "napi build --platform --release --no-const-enum",
    "build:debug": "napi build --platform --no-const-enum",
    "docs": "npm run build:debug && typedoc --plugin typedoc-plugin-mdn-links ./index.d.ts --out ./docs",
    "prepublishOnly": "napi prepublish -t npm --no-gh-release",
    "test": "node ./test/e2e/run-e2e-tests.mjs",
    "universal": "napi universal",
    "copy-version": "napi version"
  },
  "packageManager": "yarn@4.10.3",
  "description": "Impit for JavaScript",
  "optionalDependencies": {
    "impit-darwin-arm64": "0.7.1",
    "impit-darwin-x64": "0.7.1",
    "impit-linux-arm64-gnu": "0.7.1",
    "impit-linux-arm64-musl": "0.7.1",
    "impit-linux-x64-gnu": "0.7.1",
    "impit-linux-x64-musl": "0.7.1",
    "impit-win32-arm64-msvc": "0.7.1",
    "impit-win32-x64-msvc": "0.7.1"
  }
}
